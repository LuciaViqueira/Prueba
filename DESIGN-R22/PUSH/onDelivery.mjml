{ 
    "imageUrl":"", 
    "linkUrl":"[[#assign \"orderId\"]][[payload.order.identifier]][[/assign]]
        [[#if payload.order.suborders.[0].uuid]]
            [[#assign \"suborderIdNotSub8\"]][[payload.order.suborders.[0].uuid]][[/assign]]
        [[else]]
            [[#assign \"suborderIdNotSub8\"]][[payload.order.suborders.[0].suborderId]][[/assign]]
        [[/if]]
        [[def \"size\" 8]][[#def \"uuidSize\"]][[size suborderIdNotSub8]][[\/def]]
        [[#ifb (compare uuidSize \">\" size)]]
            [[#def \"index\"]][[math  uuidSize '-' size]][[\/def]]
            [[#def \"suborderId\"]][[substringb suborderIdNotSub8 index]][[\/def]]
        [[else]]
            [[def \"suborderId\" suborderIdNotSub8]]
        [[\/ifb]]
        [[#if payload.user.isGuest]]
            [[#assign \"accountToken\"]][[payload.user.accountToken]][[/assign]]
            [[urlResolverWithVersion 'OrderDetailGuestURL' 2]]
        [[else]]
            [[urlResolverWithVersion 'OrderDetailURL' 5]]
        [[/if]]", 
    "template":"
        [[#if payload.order.isSplitted]]
            [[tags.onDeliverySplittedPush]]
        [[else]]
            [[tags.onDeliveryPush]]
        [[/if]]"
}