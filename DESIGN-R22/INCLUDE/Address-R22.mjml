<mj-text padding-top="4px" css-class="rd-subsection-text">
    <div>
        [[#if this.line]]
            <div>[[this.line]]</div>
        [[/if]]
    </div>
    <div>
        [[#if this.zipCode.value]]
            [[this.zipCode.value]] - 
        [[/if]]
        [[#if this.city.value]]
            [[this.city.value]][[#if this.district.value]],[[/if]]
        [[/if]]
        [[#if this.district.value]]
            [[this.district.value]]
        [[/if]]
    </div>
</mj-text>